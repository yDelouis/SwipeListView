<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="fr.ydelouis.stdlvsample"
	android:versionCode="1"
	android:versionName="1.0">

	<uses-sdk android:minSdkVersion="8" android:targetSdkVersion="17" />

	<application
		android:allowBackup="true"
		android:icon="@drawable/ic_launcher"
		android:label="@string/swipeToDeleteListViewDemo"
		android:theme="@style/AppTheme">

		<activity
			android:name="fr.ydelouis.swipelistviewsample.MainActivity"
			android:label="@string/swipeToDeleteListView">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>

		<activity
			android:name="fr.ydelouis.swipelistviewsample.WithoutConfirmActivity"
			android:label="@string/withoutConfirm" />
		<activity
			android:name="fr.ydelouis.swipelistviewsample.WithConfirmActivity"
			android:label="@string/withConfirm" />

	</application>

</manifest>